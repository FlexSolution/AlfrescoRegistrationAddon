<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>




    <bean id="genPassword" parent="baseJavaScriptExtension" class="PasswordGenerator.PasswordGenerator">
        <property name="extensionName">
            <value>passGenerator</value>
        </property>
    </bean>


    <bean id="${project.artifactId}.workflowBootstrap" parent="workflowDeployer">
        <property name="workflowDefinitions">
            <list>
                <props>
                    <prop key="engineId">activiti</prop>
                    <prop key="location">alfresco/module/${project.artifactId}/workflows/userReviewingWorkflow.bpmn20.xml</prop>
                    <prop key="mimetype">text/xml</prop>
                    <prop key="redeploy">true</prop>
                </props>
            </list>
        </property>

        <property name="models">
            <list>
                <value>alfresco/module/${project.artifactId}/model/fs-newUserReviewWorkflow-model.xml</value>
            </list>
        </property>
    </bean>

</beans>
